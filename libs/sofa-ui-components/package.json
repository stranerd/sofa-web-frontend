{
  "name": "sofa-ui-components",
  "version": "0.0.27",
  "description": "UI components for SOFA",
  "author": "",
  "scripts": {
    "serve": "webpack serve",
    "build": "webpack --mode=production --node-env=production && npm run build:style",
    "build:dev": "webpack --mode=development",
    "build:lib": "rimraf dist && npm run build:vue && npm run build:style",
    "build:prod": "webpack --mode=production --node-env=production && npm run build:style",
    "build:style": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.css  && cleancss -o dist/library.min.css dist/library.css",
    "build:style:watch": "npx tailwindcss -i  ./src/styles/index.css -o  dist/library.min.css --watch",
    "publish:lib": "npm run build:lib && npm publish",
    "watch": "webpack --watch"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@editorjs/checklist": "^1.4.0",
    "@editorjs/editorjs": "^2.26.5",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/simple-image": "^1.5.1",
    "@vue/compiler-sfc": "^3.2.45",
    "chart.js": "^4.3.0",
    "gsap": "^3.12.2",
    "moment": "^2.29.4",
    "pdf.js-viewer": "^0.2.8",
    "swiper": "^8.4.2",
    "video.js": "^8.3.0",
    "vue": "^3.2.21",
    "vue-loader": "^17.0.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@tailwindcss/typography": "^0.5.0",
    "@types/editorjs__header": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.14",
    "clean-css-cli": "^5.6.2",
    "css-loader": "^6.7.3",
    "cypress": "^8.7.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.21",
    "rimraf": "^3.0.2",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwind-scrollbar": "^3.0.0",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "typescript": "^4.3.5",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
