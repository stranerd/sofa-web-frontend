{
  "name": "sofa",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 18.18.0"
  },
  "scripts": {
    "predev": "pnpm run bin:project:env",
    "dev": "vite",
    "dev:android": "ionic cap run android --livereload --external --port 8080",
    "dev:ios": "ionic cap run ios --livereload --external --port 8080",
    "prebuild:web": "pnpm run bin:project:env",
    "build:web": "vite build",
    "prebuild:app": "pnpm run bin:project:env",
    "build:app": "SKIP_COMPRESSION=1 vite build",
    "postbuild:app": "cap sync",
    "preview": "vite preview",
    "lint:fix": "eslint --ext ts,vue --fix ./ && echo Done",
    "lint:staged": "lint-staged",
    "prepare": "husky",
    "postinstall": "pnpm exec jetify",
    "bin:project:setup": "tsx bin/project.ts setup",
    "bin:project:version": "tsx bin/project.ts version",
    "bin:project:env": "tsx bin/project.ts env",
    "bin:project:appBuild": "pnpm run build:app && tsx bin/project.ts appBuild",
    "bin:icons": "capacitor-assets generate --ios --android",
    "bin:icons:types": "tsx bin/project.ts iconsTypes"
  },
  "lint-staged": {
    "*": "npm run lint:fix"
  },
  "dependencies": {
    "@capacitor-community/apple-sign-in": "^6.0.0",
    "@capacitor-community/file-opener": "^6.0.0",
    "@capacitor/android": "^6.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/clipboard": "^6.0.0",
    "@capacitor/core": "^6.1.0",
    "@capacitor/dialog": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.1.0",
    "@capacitor/keyboard": "^6.0.1",
    "@capacitor/preferences": "^6.0.1",
    "@capacitor/push-notifications": "^6.0.1",
    "@capacitor/share": "^6.0.1",
    "@capacitor/splash-screen": "^6.0.1",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/toast": "^6.0.1",
    "@codetrix-studio/capacitor-google-auth": "3.4.0-rc.4",
    "@tato30/vue-pdf": "^1.9.5",
    "@videojs-player/vue": "^1.0.0",
    "axios": "^1.6.8",
    "capacitor-blob-writer": "^1.1.16",
    "chart.js": "^4.4.2",
    "gsap": "^3.12.5",
    "mathlive": "^0.98.6",
    "qrcode.vue": "^3.4.1",
    "socket.io-client": "^4.7.5",
    "string-similarity": "^4.0.4",
    "valleyed": "^4.2.11",
    "video.js": "^8.10.0",
    "vue": "^3.4.21",
    "vue-draggable-plus": "^0.4.0",
    "vue-inline-svg": "^3.1.2",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.3.0",
    "vue-tel-input": "^8.3.1",
    "vue3-editor": "^0.1.1",
    "vue3-marquee": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trapezedev/project": "^7.0.10",
    "@types/node": "^20.11.30",
    "@types/plist": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "jetifier": "^2.0.0",
    "lint-staged": "^15.2.2",
    "plist": "^3.1.0",
    "postcss": "^8.4.37",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.1",
    "terser": "^5.29.2",
    "tsx": "^4.15.9",
    "typescript": "^5.4.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-watch": "^0.3.1",
    "vue-tsc": "^1.8.27"
  }
}